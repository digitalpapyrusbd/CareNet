{
  "name": "caregiver-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:frontend": "jest --testPathIgnorePatterns='backend/'",
    "test:unit": "jest --testPathPattern='__tests__/(components|lib|hooks)'",
    "test:integration": "jest --testPathPattern='__tests__/(flows|user-flows)'",
    "test:performance": "jest --testPathPattern=performance",
    "test:security": "jest --testPathPattern=security",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:a11y": "jest --runTestsByPath src/__tests__/accessibility.test.tsx",
    "test:playwright": "playwright test",
    "test:load": "artillery run tests/load/load-test-config.yml",
    "test:all": "npm run test:performance && npm run test:security && npm run test:accessibility && npm run test:load",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "ci": "npm run lint && npm run type-check && npm run test:ci",
    "analyze": "ANALYZE=true npm run build",
    "build:production": "next build",
    "build:analyze": "npm run analyze",
    "deploy": "npm run build && npm run start",
    "perf:audit": "npm run test:performance && npm run analyze",
    "security:audit": "npm audit && npm run test:security"
  },
